{
  "name": "shadcn-logos",
  "version": "0.1.0",
  "description": "A CLI tool for adding company logos to your projects",
  "type": "module",
  "bin": {
    "shadcn-logos": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist/**/*",
    "data/**/*",
    "logos/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/cli/index.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "test": "vitest",
    "prepublishOnly": "npm run build",
    "postpublish": "echo 'ðŸŽ‰ Successfully published shadcn-logos!'",
    "release": "npm version patch && npm publish"
  },
  "keywords": [
    "cli",
    "logos",
    "svg",
    "shadcn",
    "icons",
    "react",
    "vue",
    "svelte",
    "components",
    "developer-tools",
    "frontend",
    "ui"
  ],
  "author": "shadcn-logos contributors",
  "license": "MIT",
  "homepage": "https://github.com/albert-mr/shadcn-logos#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/albert-mr/shadcn-logos.git"
  },
  "bugs": {
    "url": "https://github.com/albert-mr/shadcn-logos/issues"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "picocolors": "^1.1.1",
    "ora": "^8.1.0",
    "cli-progress": "^3.12.0",
    "inquirer": "^12.0.0",
    "zod": "^3.23.8",
    "svgo": "^3.3.2",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/cli-progress": "^3.11.6",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "vitest": "^2.1.2",
    "eslint": "^9.12.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18"
  }
}
